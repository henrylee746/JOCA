{
  "kind": "collectionType",
  "collectionName": "elections",
  "info": {
    "singularName": "election",
    "pluralName": "elections",
    "displayName": "Elections"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true,
      "unique": false
    },
    "time": {
      "type": "time",
      "required": true
    },
    "location": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Executive",
        "Committee",
        "Referendum"
      ]
    },
    "isPublic": {
      "type": "boolean"
    },
    "votingDateStart": {
      "type": "date",
      "required": true
    },
    "votingDateEnd": {
      "type": "date",
      "required": true
    },
    "ballotUrl": {
      "type": "string"
    },
    "candidates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::member.member",
      "inversedBy": "elections"
    }
  }
}
