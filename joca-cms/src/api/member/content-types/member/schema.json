{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "maxLength": 50,
      "required": true
    },
    "lastName": {
      "type": "string",
      "maxLength": 50,
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "minLength": 10,
      "maxLength": 15
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "member"
    },
    "elections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::election.election",
      "mappedBy": "candidates"
    }
  }
}
